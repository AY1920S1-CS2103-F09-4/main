<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="informationHolder" alignment="CENTER_LEFT" minHeight="105" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="5" left="15" right="5" top="5" />
   </padding>
   <children>
      <Label fx:id="name" styleClass="cell_big_label" text="\$first" />
      <FlowPane fx:id="tags" />
      <FlowPane fx:id="criteria" />
      <Label fx:id="description" styleClass="cell_small_label" text="\$description" />
      <Label fx:id="coverage" styleClass="cell_small_label" text="\$coverage" />
      <Label fx:id="price" styleClass="cell_small_label" text="\$price" />
   </children>
</VBox>
